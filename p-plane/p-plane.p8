--map
plants={}
obs={}
blns={80,81,82}
bln=0

function init_map()
	for i=0,20 do
		add(plants, add_plant(rnd(120)))
	end
end

function update_map()
	if((t)%3==0 and #plants<20) then
	 add(plants, add_plant(-4))
	end
	
	if((t)%10==0) then
	 bln=bln<#blns and bln+1 or 1
	 add(obs, add_bln(bln)) 
	end
	for p in all(plants) do
		p.y+=2
		if(p.y>128) del(plants, p)
	end
	for o in all(obs) do
		o.y+=2-o.ys
		o.x+=o.xs
		if(o.hit) then
		 del(obs,o)
		end
		if(o.y>128 or o.x<-8 or o.x>128) del(obs, o)
	end
end

function draw_map()
	for p in all(plants) do
		spr(p.sp, p.x, p.y, 1, 1, p.fx, p.fy)
	end
end

function draw_obs_shadow()
	for o in all(obs) do
	 circfill(o.x-12, o.y+20, 2, 5)
	end
end

function draw_obs()
	for o in all(obs) do
		if(o.flash>0) then
		 o.flash-=1
		 for i=1,15 do
		   pal(i,7)
		  end
		end
		spr(o.sp, o.x, o.y)
		pal()
	end
end

function add_plant(y)
	return {
	 	sp=flr(rnd(3)+0.5)+64,
	 	x=rnd(120),
	 	y=y,
	 	fx=rnd(1)>0.5 and true or false,
	 	fy=rnd(1)>0.5 and true or false
	 }
end

function add_bln(bl)
 local hit_count=bl==1 and 100 or 4-bl
 sssg=hit_count
 return {
   w=8,
   h=8,
	 	sp=blns[bl],
	 	x=rnd(120),
	 	y=-4,
	 	xs=rnd(0.4)-0.2,
	 	ys=rnd(0.4),
	 	hit=false,
	 	hit_c=hit_count,
	 	flash=0,
	 	bl=bl
	 }
end
