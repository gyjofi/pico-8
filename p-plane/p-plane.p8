--main
my=0
t=0

function _init()
 init_map()
	init_plane()
end

function _update()
 t=t<60*30 and t+1 or 0
 update_map()
 update_plane()
 update_bullets()
 check_cols()
end

function _draw()
 cls(3)
 draw_map()
 draw_plane_shadow()
 draw_obs_shadow()
 draw_obs()
 draw_bullets()
 draw_plane()
 draw_info()
 print(btn()&0b1111, 0, 8, 7)
 print(t/30, 0, 16, 7)
 print(plane.health, 0, 24, 7)
 print(sssg, 0, 32, 7)
end
