--utils
health_cols={8,9,11}
function psgn(n)
	return n==0 and 0 or sgn(n)
end

function col(a,b)
    if a==nil or b==nil then 
        return false
    end
    
    local a_left=a.x
    local a_top=a.y
    local a_right=a.x+a.w-1
    local a_bottom=a.y+a.h-1
    
    local b_left=b.x
    local b_top=b.y
    local b_right=b.x+b.w-1
    local b_bottom=b.y+b.h-1

    if a_left>b_right then return false end
    if b_left>a_right then return false end
    if a_top>b_bottom then return false end
    if b_top>a_bottom then return false end
    
    return true
end

function draw_info()
 --health
	rectfill(1,1,63,4,7)
	rectfill(2,2,62,3,1)
	local col=(plane.health\30)+1
	col=mid(1,col,3)
	rectfill(2,2,2+ceil((60/100)*plane.health),3,health_cols[col])
	--track
	
end

function check_cols()
	for o in all(obs) do
		if(col(o,{
			x=plane.x+2,
			y=plane.y+2,
			w=plane.w-4,
			h=plane.h-4,
		})) then
			o.hit=true
			o.flash=1
			plane.hit=true
			plane.flash=1
			plane.health-=(3-o.bl)*5
		end
		
		for b in all(bullets) do
			if(col(o,{
				x=b.x+(b.f and 5 or 2),
				y=b.y,
				w=3,
				h=6,
			})) then
				o.flash=1
				o.hit=o.hit_c-1==0
				o.hit_c-=1
				del(bullets, b)
			end
		end
	end
end


