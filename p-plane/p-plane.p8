--bullets
bullets={}
mfs={}

function update_bullets()
	for b in all(bullets) do
		b.y-=7
		if(b.y<-8) del(bullets, b) 
	end
	for m in all(mfs) do
		m.t+=1
		if(m.t>3) del(mfs, m) 
	end

	if(bullets==0 or (btn(5) and flr(pp)==3)) then
		add(bullets, {
		 x=plane.x,
		 y=plane.y-4,
		 f=false
		})
		add(bullets, {
		 x=plane.x+8,
		 y=plane.y-4,
		 f=true
		})
		add(mfs, {
		 t=1
		})
		sfx(1)
	end
end

function draw_bullets()
	for b in all(bullets) do
	 spr(29,b.x,b.y,1,1,b.f)
	end
	for m in all(mfs) do
	 spr(m.t+31,plane.x-2,plane.y-3,1,1)
	 spr(m.t+31,plane.x+10,plane.y-3,1,1,1)
	end
end
