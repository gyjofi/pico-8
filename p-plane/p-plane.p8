--plane
plane={
 w=16,
 h=16,
 sps={1,3,5,7,9},
	sp=0,
	x=56,
	y=100,
	sx=0,
	sy=0,
	health=100,
	hit=false,
	flash=0
}

prop={11,27,13}
pp=1

butarr={1,2,0,3,5,6,3,4,8,7,4,0,1,2,0}

dirx={-1,1, 0,0, -0.7, 0.7,0.7,-0.7}
diry={ 0,0,-1,1, -0.7,-0.7,0.7,0.7}

butarr[0]=0

function init_plane()
	spd=2.4
 lastdir=0
 sfx(0)
end

function update_plane()
	if(plane.hit) then
	 plane.hit=false
	end
 
 local dir=butarr[btn()&0b1111]
 
 if lastdir!=dir and dir>=5 then
  --anti-cobblestone
  plane.x=flr(plane.x)+0.5
  plane.y=flr(plane.y)+0.5
 end
 
 local dplsp=0
 
 if dir>0 then
  plane.x+=dirx[dir]*spd
  plane.y+=diry[dir]*spd
  dplsp=psgn(dirx[dir])
 end
 
 plane.x=mid(0,plane.x,112)
 plane.y=mid(16,plane.y,112)
 
 plane.sp+=psgn(dplsp-plane.sp)*0.2
 plane.sp=mid(-1,plane.sp,1)
 
 lastdir=dir
 
 pp+=0.75
 if(pp>=4) pp=1
end

function draw_plane_shadow()
 spr(35,plane.x-12,plane.y+20,2,2)
end

function draw_plane()
	
	if(plane.flash>0) then
	 plane.flash-=1
	 for i=1,15 do
    pal(i,7)
   end
 end
 spr(plane.sps[flr(plane.sp*2.4+3.5)],plane.x,plane.y,2,2)
 pal()
 
 spr(prop[(t\1.5)%3+1],plane.x,plane.y-8,2,1)
 if(plane.health<60) then
  for i=1,10 do
   pset(plane.x+5+rnd(5), plane.y+16+rnd(12), flr(rnd(2)+5))
  end
 end
 if(plane.health<30) then
  for i=1,8 do
   pset(plane.x+5-rnd(3), plane.y+3+rnd(12), flr(rnd(3)+8))
   pset(plane.x+11+rnd(3), plane.y+3+rnd(12), flr(rnd(3)+8))
  end
 end
end

